FoamFile
{
    version     2.0;
    format      ascii;
    class       volSymmTensorField;
    object      R;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0]; // m^2/s^2

internalField   uniform (0.025 0 0 0.025 0 0.025);

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           uniform (0.025 0 0 0.025 0 0.025);
    }

    outlet
    {
        type            zeroGradient;
    }

    walls
    {
        type            kqRWallFunction;
        value           uniform (0.025 0 0 0.025 0 0.025);
    }
}

// ************************************************************************* //

